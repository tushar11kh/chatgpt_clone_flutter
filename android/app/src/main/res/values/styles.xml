<?xml version="1.0" encoding="utf-8"?>
<resources>
    <!-- Launch screen theme -->
    <style name="LaunchTheme" parent="@android:style/Theme.Black.NoTitleBar">
        <item name="android:windowBackground">@android:color/transparent</item>
